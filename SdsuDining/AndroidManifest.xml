        <activity
            android:name="sdsu.apps.sdsudining.CouponsActivity"
            android:label="@string/couponsString" >
        </activity>
        <activity
            android:name="sdsu.apps.sdsudining.PlainTestActivity"
            android:label="@string/title_activity_plain_test" >
        </activity>
        <activity
            android:name="sdsu.apps.sdsudining.PlainTestListItemActivity"
            android:label="@string/title_activity_plain_test_list_item" >
        </activity>
        <activity
            android:name="sdsu.apps.sdsudiningfragements.CateringContactFragementActivity"
            android:label="@string/title_activity_catering_contact" >
        </activity>
        <activity
            android:name="sdsu.apps.sdsudiningfragements.DisplayInfoFragment"
            android:label="@string/title_activity_display_info_fragment" >
        </activity>
    </application>

</manifest>
